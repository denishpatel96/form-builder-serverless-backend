    "rimraf": "^5.0.0",
    "serverless": "^3.30.1",
    "serverless-custom-packaging-plugin": "^0.1.2",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
